{
    "name": "powerkernel/yii-common",
    "description": "Yii Common",
    "type": "yii-module",
    "keywords": ["yii","common"],
    "license": "Private",
    "authors": [
        {
            "name": "Harry Tang",
            "email": "harry@powerkernel.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "3.0.x-dev"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "powerkernel\\yiicommon\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}